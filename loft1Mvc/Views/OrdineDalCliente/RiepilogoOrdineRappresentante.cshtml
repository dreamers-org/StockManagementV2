        <a asp-controller="OrdineDalCliente" asp-action="Create" class="btn rounded-pill btn-outline-secondary float-left mt-3"><i class="fas fa-arrow-left"></i>&nbsp;Indietro</a>
    </div>
    <div class="col-sm-4">
        <h1>@ViewData["Title"]</h1>
    </div>
    <div class="col-sm-4">
        <a asp-controller="OrdineDalCliente" asp-action="ConclusioneOrdineRappresentante" class="btn rounded-pill btn-outline-secondary float-right mt-3">Prosegui&nbsp;<i class="fas fa-arrow-right"></i></a>
    </div>
    <div class="col-sm-12">
        <hr />
    </div>
</div>

<partial name="_ListaOrdiniDalClientePartial" />


